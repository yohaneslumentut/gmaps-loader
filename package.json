{
  "name": "gmaps-script-loader",
  "version": "0.1.2",
  "license": "MIT",
  "author": "Yohanes O.Lumentut",
  "homepage": "https://github.com/yohaneslumentut/gmaps-script-loader/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/yohaneslumentut/gmaps-script-loader"
  },
  "description": "A simple react hook for loading a google maps API script on browser",
  "keywords": [
    "google",
    "maps"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint src",
    "prepare": "tsdx build && husky install",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/gmaps-script-loader.esm.js",
  "size-limit": [
    {
      "path": "dist/gmaps-script-loader.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gmaps-script-loader.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@size-limit/webpack": "^8.1.0",
    "@size-limit/webpack-why": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/google.maps": "^3.50.0",
    "@types/jsdom": "^21.1.6",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "husky": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.1.0",
    "ts-jest": "^25.3.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^3.7.3"
  }
}
